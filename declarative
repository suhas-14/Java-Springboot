pipeline {
	agent any
stages {
	stage('Fetch the code')
	{
		steps {
			git 'https://github.com/suhas-14/Java-Springboot.git'
		      }
	}
	stage('Build')
	{
		steps {
			sh 'mvn clean install'
		      }
	}
	stage('Result')
	{
		steps {
			echo "I have completed fethcing the code and running the maven job"
		      }
	}
}
}
